{
  "name": "mytable",
  "version": "0.19.4",
  "description": "Mytable is a web app that complements Airtable workflow with scripts that enable organizing items in a tree structure and automate the basic creation of recursive tasks.",
  "keywords": [
    "airtable",
    "firebase",
    "productivity",
    "recursive-tasks",
    "tree-structure"
  ],
  "homepage": "https://github.com/joaomelo/mytable",
  "bugs": "https://github.com/joaomelo/mytable/issues",
  "license": "GPL-3.0",
  "author": "jo√£o melo <j@melo.plus> (https://blog.melo.plus)",
  "repository": {
    "type": "git",
    "url": "https://github.com/joaomelo/mytable.git"
  },
  "scripts": {
    "start": "npm run dev:client",
    "dev:client": "webpack-dev-server --config client/cfg/webpack.dev.js",
    "lint": "eslint --fix --ext .js,.vue client/src",
    "commit": "git add -A && git commit",
    "push": "git push && git push --tags",
    "bump:patch": "npm run lint && npm run commit && npm version patch && npm run push",
    "bump:minor": "npm run lint && npm run commit && npm version minor && npm run push",
    "bump:major": "npm run lint && npm run commit && npm version major && npm run push",
    "build": "rimraf client/dist/* && webpack --config client/cfg/webpack.prod.js",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@joaomelo/auth-mech": "^0.1.21",
    "@joaomelo/hot-collection": "^0.8.18",
    "airtable": "^0.8.1",
    "bottleneck": "^2.19.5",
    "core-js": "^3.6.5",
    "firebase": "^7.15.0",
    "moment": "^2.29.4",
    "regenerator-runtime": "^0.13.5",
    "rxjs": "^6.5.5",
    "vue": "^2.6.11",
    "vue-router": "^3.3.2",
    "vuetify": "^2.2.32"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "deepmerge": "^4.2.2",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-libs": "^4.0.0",
    "favicons-webpack-plugin": "^3.0.1",
    "fibers": "^4.0.3",
    "first-input-delay": "^0.1.3",
    "html-webpack-plugin": "^4.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
